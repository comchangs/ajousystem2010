			android:layout_x="85px"
			android:layout_y="4px"
			android:layout_width="wrap_content"
			android:id="@+id/tv_Temperature01"
			android:textSize="10px"
			android:text="@string/str_temperature1"></TextView>
		<TextView
			android:layout_height="wrap_content"
			android:layout_x="85px"
			android:layout_y="22px"
			android:layout_width="wrap_content"
			android:textSize="10px"
			android:text="@string/str_temperature1"
			android:id="@+id/tv_Humidity01"></TextView>
		<ImageView
