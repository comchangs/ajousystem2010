			android:layout_y="42px"
			android:id="@+id/Dipsw1"
			android:baselineAlignBottom="false"
			android:clickable="false"
			android:layout_height="16px"
			android:layout_width="43px"
			android:background="@drawable/swon"></ImageView>
		<ImageButton
